{
	"expo": {
		"name": "QuickWeather",
		"version": "1.1.0",
		"slug": "QuickWeather",
		"description": "QuickWeather, seu aplicativo de clima simples e amig√°vel.",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "quickweather",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"platforms": [
			"android",
			"web"
		],
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"favicon": "./assets/images/favicon.png"
		}
	}
}